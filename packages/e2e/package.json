{
  "name": "@minhas-financas/e2e",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "cypress run",
    "sso:open": "cypress open --config baseUrl=http://localhost:4200",
    "sso": "cypress run --config baseUrl=http://localhost:4200 --spec 'test/sso/**/*'",
    "manager:open": "cypress open --config baseUrl=http://localhost:4201",
    "manager": "cypress run --config baseUrl=http://localhost:4201 --spec 'test/manager/**/*'"
  },
  "dependencies": {
    "cypress": "^13.16.0",
    "cypress-xpath": "^2.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "cross-env": "^7.0.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "packageManager": "yarn@3.2.1"
}